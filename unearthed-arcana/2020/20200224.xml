			<div element="ID_WOTC_UA20200224_ARCHETYPE_FEATURE_FEY_WANDERER_BEGUILING_TWIST" />
			<div element="ID_WOTC_UA20200224_ARCHETYPE_FEATURE_FEY_WANDERER_MISTY_PRESENCE" />
		</description>
		<sheet display="false" />
		<rules>
			<grant type="Archetype Feature" id="ID_WOTC_UA20200224_ARCHETYPE_FEATURE_FEY_WANDERER_FEY_WANDERER_MAGIC" level="3" />
