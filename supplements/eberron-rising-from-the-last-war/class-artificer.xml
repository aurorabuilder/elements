			<grant type="Class Feature" id="ID_WOTC_ERLW_CLASS_FEATURE_ARTIFICER_SPELL_STORING_ITEM" level="11" />
			<grant type="Class Feature" id="ID_WOTC_ERLW_CLASS_FEATURE_ARTIFICER_MAGIC_ITEM_SAVANT" level="14" />
			<grant type="Class Feature" id="ID_WOTC_ERLW_CLASS_FEATURE_ARTIFICER_MAGIC_ITEM_MASTER" level="18" />
