	<element name="Burglar’s Pack" type="Item" source="Player’s Handbook" id="ID_WOTC_ITEM_BURGLARS_PACK">
		<description>
			<p>Includes a backpack, a bag of 1,000 ball bearings, 10 feet of string, a bell, 5 candles, a crowbar, a hammer, 10 pitons, a hooded lantern, 2 flasks of oil, 5 days rations, a tinderbox, and a waterskin. The pack also has 50 feet of hempen rope strapped to the side of it.</p>
		</description>
		<setters>
			<set name="category">Equipment Packs</set>
			<set name="cost" currency="gp">16</set>
			<set name="weight" lb="10">10 lbs.</set>
		</setters>
		<extract>
			<item>ID_WOTC_PHB_ITEM_BACKPACK</item>
			<item>ID_WOTC_PHB_ITEM_BALLBEARINGS</item>
			<item>ID_WOTC_PHB_ITEM_STRING_10FEET</item>
			<item>ID_WOTC_PHB_ITEM_BELL</item>
			<item amount="5">ID_WOTC_PHB_ITEM_CANDLE</item>
			<item>ID_WOTC_PHB_ITEM_CROWBAR</item>
			<item>ID_WOTC_PHB_ITEM_HAMMER</item>
			<item amount="10">ID_WOTC_PHB_ITEM_PITON</item>
			<item>ID_WOTC_PHB_ITEM_LANTERN_HOODED</item>
			<item amount="2">ID_WOTC_PHB_ITEM_OIL_FLASK</item>
			<item amount="5">ID_WOTC_PHB_ITEM_RATIONS_1DAY</item>
			<item>ID_WOTC_PHB_ITEM_TINDERBOX</item>
			<item>ID_WOTC_PHB_ITEM_WATERSKIN</item>
			<item>ID_WOTC_PHB_ITEM_ROPE_HEMPEN_50FEET</item>
		</extract>
	</element>
