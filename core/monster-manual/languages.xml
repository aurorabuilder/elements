	<!-- primordial language can communicate with these dialects -->
	<element name="Auran" type="Language" source="Monster Manual" id="ID_MM_LANGUAGE_AURAN">
		<description>
			<p>Typically spoken by elementals.</p>
		</description>
		<setters>
			<set name="exotic">true</set>
			<set name="speakers">Air elementals</set>
			<set name="script"></set>
		</setters>
	</element>
	<element name="Terran" type="Language" source="Monster Manual" id="ID_MM_LANGUAGE_TERRAN">
